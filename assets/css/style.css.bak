/*********** Hero ***********/
.hero {
  background-image: url(../img/hero.jpg);
  background-size: cover;
  background-position: bottom;
  margin:0px auto;
  border-top: 1px solid #d8e3fe;
}

.hero__subtitle {
  color: #3D4F9F;
  font-family: Roboto;
  font-weight: 600;
  font-size: 10px;
  display: flex;
  justify-content: center;
  line-height: 12px;
  letter-spacing: 0.1em;
  align-items: center;
  padding-top:92px;
}



.slider span {
  display: inline-block;
  width: 10px;
  height: 10px;
  border-radius: 50%;
  background-color: #D1D6E3;
  cursor: pointer;
}
  
.slider span:not(:last-child){
  margin: right 8px;
}

.hero__title {
    display: flex;
    justify-content: center;
    margin-top: 16px;
	color: #3D4F9F;
	font-weight: 300;
}

.hero__text {
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 15px;
    line-height: 29px;
    text-align: center;
    letter-spacing: 0.03em;
    color: #8D96BD;
    margin-top: 28px;
}

button.btn.btn--play {
    display: flex;
    border-radius: 50%;
    width: 52px;
	margin: 20px auto;
	background: #FFFFFF;
	box-shadow: 0px 10px 50px rgba(201, 209, 231, 0.6);
	border-color:transparent;
}

.hero__slider {
  text-align: center;
  margin: 176px 0px 28px 0px;
}

/*********** partners-section ***********/
.partners-section {
  padding: 58px 0px;
}

.partners-section .container {
  display: flex;
  justify-content: center;
  align-items: baseline;
  flex-wrap: wrap;
  flex-direction: row;
  gap: 10px 20px;
}

a.partner {
  color: #D6DAE5;
}

a.partner:hover {
  color: #5982E7;
}
