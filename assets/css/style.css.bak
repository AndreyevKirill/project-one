@font-face {
  font-family: "Roboto";
  src: url("/assets/fonts/Roboto-Regular.ttf") format("truetype");
  font-style: normal;
  font-weight: normal;
}
@font-face {
  font-family: "Quicksand";
  src: url("../fonts/Quicksand-Regular.ttf") format("truetype");
  font-style: normal;
  font-weight: normal;
}

html,
body {
  margin: 0;
  padding: 0;
}

body {
  font-family: "Roboto", "Arial", sans-serif;
  font-size: 15px;
  line-height: 29px;
}

h1,
h2,
h3,
h4,
h5,
h6 {
  font-family: "Quicksand", "Arial", sans-serif;
}

h2 {
  font-size: 48px;
  font-family: Quicksand;
  font-style: normal;
  font-weight: 300;
  font-size: 48px;
  line-height: 60px;
  text-align: center;
  color: #ffffff;
}
p {
  letter-spacing: 0.03em;
}

/*********** header ***********/
.header {
    background: #FFFFFF;
    height: 80px;
}
.header__left,
.header__right {
    display: flex;
    align-items: center;
}

.header .container {
    display: flex;
    align-items: center;
    height: 100%;
    justify-content: space-between;
}

.logo {
    height: 26px;
    width: 26px;
    background-color: #5E81FE;
    border: 0px;
    border-radius: 50%;
    margin-right: 73px;
}

.menu-trigger {
margin-right: 44px;
}
.menu-trigger,
.menu-trigger span {
	display: inline-block;
	transition: all .4s;
	box-sizing: border-box;
}

.menu-trigger {
	position: relative;
	width: 50px;
	height: 44px;
}

.menu-trigger span {
	position: absolute;
	left: 0;
	width: 100%;
	height: 4px;
	background-color: #5E81FE;
	border-radius: 4px;
}

.menu-trigger span:nth-of-type(1) {
	top: 0;
}

.menu-trigger span:nth-of-type(2) {
	top: 20px;
}

.menu-trigger span:nth-of-type(3) {
	bottom: 0;
}

.menu-trigger.menu-triger--active span:nth-of-type(1) {
	-webkit-transform: translateY (20px) rotate (-45deg);
	transform: translateY(20px) rotate(-45deg);
}

.menu-trigger.menu-triger--active span:nth-of-type(2) {
	opacity: 0;
}

.menu-trigger.menu-triger--active span:nth-of-type(3) {
	-webkit-transform: translateY(-20px) rotate(45deg);
	transform: translateY(-20px) rotate(45deg);
}

.menu {
    display: none;
}
.menu__item{
    color: #697CA6;
    font-weight: 600;
    text-decoration: none;
}
.menu__item:not(:last-child) {
    margin-right: 42px;
}

.menu__item:hover{
    color: #2750a7;
    cursor:pointer;
}

.menu__item--active{
    color: #3A79FF; 
}

.btn.btn--primary {
color:#3A79FF;
background: #D2E1FF;
border-radius: 2px;
border: 0px;
padding: 13px 16px;
height: 43px;   
}

.btn.btn--primary:hover {
    cursor:pointer;
}

.socials {
    display: flex;
    align-items: center;
    margin-right: 44px;
}

.social+.social {
    margin-left: 35px;
}

