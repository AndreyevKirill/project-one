/*********** customer_help ***********/
.customer_help {
    height: 964px;
    border: 1px solid;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.cus__title {
    font-size: 10px;
    line-height: 12px;
    letter-spacing: 0.1em;
    text-transform: uppercase;
    color: #3D4F9F;
    padding-top: 176px;
}

.cus__title {
    font-weight: 600;
}

.cus__subtitle {
    font-weight: 500;
    font-size: 24px;
    line-height: 30px;
    text-align: center;
    padding-top: 20px;
    color: #3D4F9F;
}

.cus__spisok {
    font-weight: 500;
    font-size: 15px;
    line-height: 19px;
    display: flex;
    align-items: center;
    letter-spacing: 0.03em;
    color: #8E96B7;
}
.cus__spisok{
	padding: 15px 0px 17px 0px;
}
.cus__spisok li{
	background-image: url(../img/li.jpg);
    background-repeat: no-repeat;
    background-position: 6px 6px;
    padding-left: 26px;
    list-style: none;
	background-size: 15px;
}

